int add(int a, int b) {
    return a + b;
}

int main() {
    int x;
    x = 10;
    int y;
    y = 5;

    if (x > y) {
        print("x is greater than y");
    } else {
        print("x is not greater than y");
    }

    int sum;
    sum = add(x, y);
    print(sum);

    int product;
    product = x * y;
    print(product);

    if (x == 10) {
        print("x is ten");
    }

    return 0;
}
