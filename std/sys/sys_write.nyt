// sys_write(fd, ptr, count) â†’ bytes written
int sys_write(int fd, string data, int count) {
    int result;
    asm {
        "mov rax, 1"
        "mov rdi, [rbp + 16]"
        "mov rsi, [rbp + 24]"
        "mov rdx, [rbp + 32]"
        "syscall"
        "mov [rbp - 4], eax"
    }
    return result;
}
